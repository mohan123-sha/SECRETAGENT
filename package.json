{
  "name": "Component-Instantiator",
  "version": "1.0.0",
  "description": "Your Figma Plugin",
  "main": "code.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --ext .ts,.tsx --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .ts,.tsx --ignore-pattern node_modules --fix .",
    "watch": "npm run build -- --watch",
    "test-gemini": "node test-gemini.js",
    "test-code-generator": "node test-code-generator.js",
    "test-code-generator-full": "node test-code-generator.js --full",
    "start-backend": "node backend-server.js",
    "start-code-server": "node code-generation-server.js",
    "start-both": "concurrently \"npm run start-backend\" \"npm run start-code-server\"",
    "storybook": "npx storybook dev -p 6006",
    "build-storybook": "npx storybook build",
    "story-server": "node story-server.js"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/animations": "^16.2.12",
    "@angular/cli": "^16.2.16",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^21.0.6",
    "@angular/compiler-cli": "^21.0.6",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^21.0.6",
    "@angular/localize": "^21.0.6",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^21.0.6",
    "@angular/service-worker": "^21.0.6",
    "@figma/eslint-plugin-figma-plugins": "*",
    "@figma/plugin-typings": "*",
    "@storybook/addon-docs": "^10.2.0-alpha.10",
    "@storybook/test": "^8.6.14",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.54.0",
    "rxjs": "^7.8.2",
    "typescript": "^5.3.2",
    "zone.js": "^0.16.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@storybook/angular": "^7.6.20",
    "cors": "^2.8.5",
    "csstype": "^3.2.3",
    "express": "^4.18.2",
    "form-data": "^4.0.5",
    "node-fetch": "^3.3.2",
    "primeicons": "^7.0.0",
    "primeng": "^21.0.2",
    "storybook": "^7.6.20",
    "undici-types": "^7.16.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@figma/figma-plugins/recommended",
      "plugin:storybook/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  }
}
